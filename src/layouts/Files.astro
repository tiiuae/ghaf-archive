---
import Base from "./Base.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Base title={title}>
  <div class="w-3xl m-auto">
    <a class="text-white no-underline hover:underline" href="/"
      ><h1 class="text-3xl my-6">Ghaf Archive</h1></a
    >
    <slot />
  </div>
</Base>
